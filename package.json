    "mongoose": "^5.7.5",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.3",
    "node-sass": "^7.0.0"
  }
}
